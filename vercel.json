{
    "version" : 2,
    "devCommand":"uvicorn src.index:app --host 0.0.0.0 --port 3000",
    "builds" : [
        {
        "src": "src/api/index.py",
        "use": "@vercel/python" 
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "src/api/index.py"
        }
    ]
}